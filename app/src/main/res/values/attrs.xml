<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FloatingLabelEditText">
        <attr name="lock_text" format="boolean"/>
        <attr name="input_text" format="string"/>
        <attr name="hint_text" format="string"/>
        <attr name="android:imeOptions" />
        <attr name="android:inputType" />
        <attr name="android:digits"/>
    </declare-styleable>

    <declare-styleable name="InputEditTextView">
        <attr name="hint" format="string" />
        <attr name="android:imeOptions" />
        <attr name="android:inputType" />
        <attr name="android:digits"/>
        <attr name="nextInputFocusId" format="integer"/>
        <attr name="password" format="boolean" />
    </declare-styleable>

</resources>